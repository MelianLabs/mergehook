<%= form_for @project do |f| %>
  <% f.object.errors.full_messages.each do |msg| %>
    <p><%= msg %></p>
  <% end %>

  <%= f.label :repo %>
  <%= f.text_field :repo, placeholder: "owner/repo_name" %>

  <br>

  <%= f.label :tracker_project_id %>

  <%= f.select :tracker_project_id, @tracker_projects %>

  <br>
  <%= f.submit %>
<% end %>